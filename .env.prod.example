# Production Environment Variables
# Copy this file to .env.prod and fill in your actual values

# Database
POSTGRES_PASSWORD=your_secure_password_here

# JWT Configuration
JWT_ISSUER=https://your-domain.com
JWT_AUDIENCE=https://api.your-domain.com
JWT_HS256_SECRET=your_very_secure_jwt_secret_key_here

# CORS
CORS_ORIGIN=https://your-domain.com

# OpenAI
OPENAI_API_KEY=sk-your-openai-api-key-here

# Secrets Backend
SECRETS_BACKEND=env

# Optional: Tenant Connection DEK (for development)
# TENANT_CONN_DEK_BASE64=your_base64_encoded_key_here
